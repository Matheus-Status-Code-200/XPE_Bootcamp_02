print('\nTeste de subconjunto:\n')
def subconj(conjunto, subconjunto): 
    contador = 0
    for elemento in subconjunto: 
        if elemento in conjunto: 
            contador += 1
    if contador == len(subconjunto): 
        return True 
    else:  return False

conjunto = {1, 2, 3, 4, 5, 6} 
subconjunto = {2, 4, 6} 

if(subconj(conjunto, subconjunto)): 
    print("Conjunto tem Subconjunto") 
else: 
    print("Conjunto n√£o tem um subconjunto")
def subconjunto(lista): 
    resultado = [] 
    for i in range(0, len(lista)+1): 
        for j in range(i + 1, len(lista)+1): 
            subconj = lista[i:j]
        resultado.append(subconj) 
    return resultado 
lista = [2, 3, 4, 5] 

print(subconjunto(lista),'\n')